// Accept two numbers from user and swap their values

#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    
    cout<<" enter the element a and b  "<<endl;
    cin>>a>>b;
    c=a; 
    a=b;
    b=c;
    cout<<" after swapping "<<endl;
    cout<< " a is  "<<a<<endl;
    cout<<" b is "<< b<<endl;

return 0;

}
