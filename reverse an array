// reverse code using vector and function 
#include<iostream>
#include<vector>
using namespace std;
void reversearray(int size , vector<int >&vec ){
int start =0;
int end=size-1;
while(start<end){
  swap( vec[start],vec[end]);

  start++;
  end--;
  } 
}

int main(){
vector<int>vec={ 4,3,2 };
int sizes=3;
 reversearray( sizes,vec) ;

cout<<" after reversing "<<endl;
for(int i=0;i<sizes;i++){
cout<< vec [i]  <<endl; 
   }
}
